<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ingeniousat.com.attendancetrackerr">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".AttendanceActivity" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".HistoryActivity"></activity>
        <activity android:name=".ReminderActivity"></activity>
        <activity android:name=".AddReminderActivity"></activity>
        <activity android:name=".AddReminderFirstActivity"></activity>
        <activity android:name=".AddReminderActivitySecond"></activity>
        <activity android:name=".EditReminderActivity"></activity>
        <activity android:name=".ViewingDatesActivity"></activity>
        <activity android:name=".ViewActivity"></activity>
    </application>

</manifest>